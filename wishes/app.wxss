/**app.wxss**/
page{
    width:100%;
    height:100%;
    background: #f0f0f0;
    font-size: 28rpx;
    position: relative;
}
.style-color{
    color: #ff5f13;
}
.style-bg{
    background: #ff5f13;
}
.style-btn-bg{
    color:#fff;
    background: #ff5f13;
}
.mask{
    width:100%;
    height:100%;
    z-index:2;
    position: fixed;
    top:0;
}
.shadeMask{
    display:flex;
    justify-content:center;
     align-items:center;
     width:100%;
    height:100%;
    background:rgba(0,0,0,0.6);
    z-index:989;
    position: fixed;
    top:0;
}
.query{
    width:90rpx;
    height:90rpx;
    border-radius: 50%;
    border: 1rpx solid #d8d8d8;
    background: #fff;
    text-align: center;
    line-height: 90rpx;
    position: absolute;
    bottom: 80rpx;
    right:40rpx;
    z-index: 99;
}
.clue{
    margin-right: 10rpx;
}
.info{
    margin-right: 20rpx;
}
