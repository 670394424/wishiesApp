.demand-header-detail{
    display: flex;
    margin-bottom:30rpx;
    padding: 0 40rpx;
}
.demand-header-clue{
    width:160rpx;
    line-height: 56rpx;
}
.demand-header-info input{
    width:520rpx;
    background: #fff;
}
.demand-header-time{
    background: #fff;
}
.demand-header-ipt{
    display: flex;
    width:520rpx;
    justify-content: space-between;
}
.demand-header-ipt input,.demand-header-ipt view{
    background: #fff;
    width:220rpx;
    line-height: 56rpx;
}
.demand-header-btn{
    padding: 0 40rpx 0 30rpx;
}
.stockDd-header{
    padding-top: 30rpx;
    margin-bottom: 30rpx;
}
.stockDd-main{
    padding-bottom: 200rpx;
}
.padd-top-110{
    padding-top: 110rpx!important;
}
.stock-header{
    line-height: 40rpx;
    background: #fff;
}
.stock-header-total{
    text-align: center;
}
.stock-container .clue{
    margin-right: 10rpx;
}
.stock-container .info{
    margin-right: 20rpx;
}
.stock-header-total,.stock-header-detail{
    padding: 10rpx 0;
    border-bottom: 1rpx solid #d8d8d8;
}

.stock-detail-electer view,.stock-detail-finance view,.stock-detail-business view{
    text-align: center;
    line-height: 44rpx;
}
.stock-main-info{
    margin-top: 20rpx;
    background: #fff;
}
.stock-info-top{
    display: flex;
    height:160rpx;
    padding: 0 20rpx;
}
.stock-top-id{
    display: flex;
    height:100%;
    align-items: center;
    margin-right: 20rpx;
}
.stock-top-id text{
    display: inline-block;
    width:50rpx;
    height:50rpx;
    border-radius: 50%;
    border:1rpx solid #d8d8d8;
    text-align: center;
    line-height: 50rpx;
}
.stock-top-detail{
    line-height: 46rpx;
    padding: 10rpx 0;
}
.stock-info-bottom{
    padding-bottom: 10rpx;
}
.stock-info-time,.stock-info-bank{
    display: flex;
    padding: 0 20rpx;
    flex-direction: row;
}
.stock-info-time view,.stock-info-bank view{
    width:50%;
    line-height: 46rpx;
}
.stockDd-nav{
    width:100%;
    display: flex;
    background: #fff;
    position: fixed;
    top:0;
    z-index: 994;
    border-bottom: 1rpx solid #d8d8d8;
}
.stockDd-nav-item{
    width:33.3%;
    text-align: center;
    line-height: 80rpx;
}
.nav-item-active{
    background: #ECECEC;
}
.stockDd-bottom{
    width:100%;
    padding-top: 10rpx;
    border-top: 1rpx solid #d8d8d8;
    background: #fff;
    position: fixed;
    bottom:0
}
.drop-down{
    width:520rpx;
    line-height: 56rpx;
    background: #fff;
    position: relative;
}
.drop-down:after{
    content: "";
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 5px solid #000;
    position: absolute;
    width: 0;
    top:21rpx;
    right:15rpx;
}
.w510{
    width:510rpx!important;
}
.date-sel{
    width: 100%;
    position: absolute;
    bottom:0;
    background: #fff;
}
.date-sel view{
    text-align: center;
}
.businessDay-btn{
    display: flex;
    justify-content:space-between;
    padding: 0 50rpx;
    border-bottom: 1rpx solid #E5E5E5;
}
.businessDay-btn view{
    padding: 20rpx 20rpx;
}
.businessDay-btn-cancel{
    color: #888;
}
.businessDay-btn-ensure{
    color: #1AAD19;
}
.user-sort-sel{
    width: 100%;
    position: absolute;
    bottom:0;
    background: #fff;
}
.user-sort-sel view{
    text-align: center;
}
.birthday-btn{
    display: flex;
    justify-content:space-between;
    padding: 0 50rpx;
    border-bottom: 1rpx solid #E5E5E5;
}
.birthday-btn view{
    padding: 20rpx 20rpx;
}
.birthday-btn-cancel{
    color: #888;
}
.birthday-btn-ensure{
    color: #1AAD19;
}